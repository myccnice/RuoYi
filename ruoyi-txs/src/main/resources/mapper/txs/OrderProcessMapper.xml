<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.txs.mapper.OrderProcessDao">
    <sql id="colums">
        id, orderNo, processId, appointDays, status, noticeEmpId,
        noticeTime, remark, createBy, createTime, updateBy, updateTime
    </sql>
    <select id="selectList" parameterType="TxsOrderProcess" resultType="TxsOrderProcess">
        SELECT <include refid="colums"/> FROM txs_order_process
        <where>
            <if test="orderNo != null and orderNo != ''">
                AND `orderNo`= #{orderNo}
            </if>
        </where>
    </select>
    <select id="selectById" parameterType="Long" resultType="TxsOrderProcess">
        SELECT <include refid="colums"/> FROM txs_order_process WHERE id = #{id}
    </select>
    <update id="update" parameterType="TxsOrderProcess">
        UPDATE txs_order_process
        <set>
            <if test="status != null and status != 0">status = #{status},</if>
            <if test="noticeEmpId != null and noticeEmpId != 0">noticeEmpId = #{noticeEmpId},</if>
            <if test="appointDays != null and appointDays != 0">appointDays = #{appointDays},</if>
            <if test="noticeTime != null noticeTime != ''">noticeTime = #{noticeTime},</if>
            <if test="remark != null and remark != ''">remark = #{remark},</if>
            <if test="updateBy != null and updateBy != ''">updateBy = #{updateBy},</if>
            <if test="updateTime != null">updateTime = #{updateTime},</if>
        </set>
        WHERE id = #{id}
    </update>
    <insert id="insert" parameterType="TxsOrderProcess" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO txs_order_process(<include refid="colums"/>)values(
            #{id}, #{orderNo}, #{processId}, #{appointDays}, #{status}, #{noticeEmpId},
            #{noticeTime}, #{remark}, #{createBy}, #{createTime}, #{updateBy}, #{updateTime}
        )
    </insert>
</mapper> 